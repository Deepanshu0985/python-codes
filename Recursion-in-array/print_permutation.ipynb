    "def printpermutation(s,taken):\n",
    "    if len(s)==0:\n",
    "        print(taken)\n",
    "        return\n",
    "    for i in range(0,len(taken)+1):\n",
    "        printpermutation(s[1:],taken[0:i]+s[0]+taken[i:])\n",
    "    return\n",
    "    "
   ]
